/**
 * Demultiplexor:
 * {a, b} = {in, 0} if sel == 0
 *          {0, in} if sel == 1
 */

CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    // a = in and not selection
    Not(in=sel, out=nsel);
    And(a=in, b=nsel, out=a);
    // b = in and selection
    And(a=in, b=sel, out=b);
}
